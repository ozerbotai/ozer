<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Ozer</title>
    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="../../img/favicon-light.svg" id="favicon">
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined"
      rel="stylesheet"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Exo+2:ital,wght@0,100..900;1,100..900&display=swap"
      rel="stylesheet"
    />
    <link href="../styles/styles.css" rel="stylesheet" />
    <script src="https://unpkg.com/i18next@latest/i18next.min.js"></script>
    <script src="../js/i18n-config.js"></script>
  </head>
  <body>
    <%- include('partials/header') -%>
    <input type="text" id="email" placeholder="" disabled style="display: none;" value=<%= user.emails[0].value %>>
    <main>
      <%- include('partials/connect-QR/connect-QR-loading') -%>
      <section id="section-connect-main" style="display: block;">
        <section class="container">
          <h1 class="main-title" id="connect-title-instalationOzer"></h1>
        </section>
        <section class="container">
          <div class="row install-section mb-5 mb-lg-0">
            <%- include('partials/connect-QR/connect-QR-code') -%>
            <%- include('partials/connect-QR/connect-QR-instructions') -%>
            <%- include('partials/connect-QR/connect-QR-terms-privacy-message') -%>
          </div>
        </section>
      </section>
    </main>

    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
      integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"
      integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
      crossorigin="anonymous"
    ></script>

    <script src="https://cdn.jsdelivr.net/gh/davidshimjs/qrcodejs/qrcode.min.js"></script>

    <!-- Favicon -->
    <script src="../js/favicon.js"></script>
    <!-- Language dropdown -->
    <script src="../js/language.js"></script>
    <script src="../js/connect.js"></script>
    <script src="../js/header.js"></script>
    <script src="../js/script-translations.js"></script>
    <script src="../js/cookies.js"></script>
    <script src="../js/script-data-fetch.js"></script>
    <script src="../js/script-aux-functions.js"></script>
    <script src="../js/user-event-constants-frontend.js"></script>
    <script src="../js/qrcode.min.js"></script>
    <script src="../js/jquery.min.js"></script>
    <!-- Tooltip -->
    <script src="../js/tooltip.js"></script>
  </body>
</html>
