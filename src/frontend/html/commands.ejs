<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Ozer</title>
    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="../../img/favicon-light.svg" id="favicon">
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined"
      rel="stylesheet"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Exo+2:ital,wght@0,100..900;1,100..900&display=swap"
      rel="stylesheet"
    />
    <link href="../styles/styles.css" rel="stylesheet" />
    <script src="https://unpkg.com/i18next@latest/i18next.min.js"></script>
    <script src="../js/i18n-config.js"></script>
  </head>
  <body>
    <%- include('partials/header') -%>
    <input type="text" id="email" placeholder="" disabled style="display: none;" value=<%= user.emails[0].value %>>
    <main class="main-container">
        <section class="container my-4 mt-3 mt-lg-4 sections-custom-width">
          <a
            id="linkGotoConfig"
            href="#"
            class="d-flex align-items-center gap-1 text-decoration-none mb-2"
          >
            <span class="material-symbols-outlined" aria-hidden="true">
              arrow_back
            </span>
            <span id="commands-link-backToConfig"></span>
          </a>
          <h1 class="mb-0 h4" id="commands-title-writingCommandsWhatsapp"></h1>
        </section>
  
        <section class="container sections-custom-width">
          <div class="row install-section">
            <div class="col-12 col-lg-6 install-section__item">
              <div class="sections-box">
                <span class="material-symbols-outlined" aria-hidden="true">
                  keyboard_command_key
                </span>
                <p class="h6 my-2" id="commands-title-ozerOn"></p>
                <p class="text-secondary mb-0" id="commands-paragraph-writeOzerOnChatInstructions">
                </p>
              </div>
            </div>
            <div class="col-12 col-lg-6 install-section__item">
              <div class="sections-box">
                <span class="material-symbols-outlined" aria-hidden="true">
                  keyboard_command_key
                </span>
                <p class="h6 my-2" id="commands-title-ozerOff"></p>
                <p class="text-secondary mb-0" id="commands-paragraph-writeOzerOffChatInstructions">
                </p>
              </div>
            </div>
          </div>
        </section>
    </main>
    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
      integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"
      integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
      crossorigin="anonymous"
    ></script>
    <!-- Favicon -->
    <script src="../js/favicon.js"></script>
    <!-- Language dropdown -->
    <script src="../js/language.js"></script>
    <script src="../js/commands.js"></script>
    <script src="../js/header.js"></script>
    <script src="../js/script-translations.js"></script>
    <script src="../js/cookies.js"></script>
    <!-- Tooltip -->
    <script src="../js/tooltip.js"></script>
  </body>
</html>
